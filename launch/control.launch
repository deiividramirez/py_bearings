  <rosparam command="load" file="$(find py_bearings)/config/params_iris2.yaml" ns="ns2"></rosparam>
  <rosparam command="load" file="$(find py_bearings)/config/params_iris3.yaml" ns="ns3"></rosparam>
  <rosparam command="load" file="$(find py_bearings)/config/params_iris4.yaml" ns="ns4"></rosparam>
  <rosparam command="load" file="$(find py_bearings)/config/general.yaml" ns="general"></rosparam>
</launch>
